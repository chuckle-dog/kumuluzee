FROM openjdk:8
LABEL{{#name}} name="{{name}}"{{/name}}{{#description}} description="{{description}}"{{/description}}

COPY lib /opt/lib

{{#modules}}
COPY modules/{{executableName}} /opt/lib
{{/modules}}

COPY {{executableName}} /opt

ENTRYPOINT ["java", "-jar", "/opt/{{executableName}}"]
EXPOSE {{port}}